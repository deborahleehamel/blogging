<div class="container"
<div id="main_content">
   <div id="header">
     <p>
       All the Posts
     </p>
     <div class="buttons">
        <%= link_to "Add New Post", new_post_path %>
     </div>
   </div>


<% @posts.each do |post| %>
  <div class="post_wrapper">
    <h2 class="title"> <%= link_to post.title, post %>
    </h2>
    <p class="date"> <%= post.created_at.strftime('%-b %-d, %Y') %>

    </p>

  </div>
<% end %>

</div>
